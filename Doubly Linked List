#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node *prev, *next;
};

struct Node* head = NULL;

void insertEnd(int val) {
    struct Node* newNode = malloc(sizeof(struct Node));
    newNode->data = val;
    newNode->next = NULL;
    newNode->prev = NULL;

    if (!head) {
        head = newNode;
    } else {
        struct Node* temp = head;
        while (temp->next) temp = temp->next;
        temp->next = newNode;
        newNode->prev = temp;
    }
}

void displayForward() {
    struct Node* temp = head;
    while (temp) {
        printf("%d ", temp->data);
        temp = temp->next;
    }
}

void displayBackward() {
    struct Node* temp = head;
    while (temp && temp->next) temp = temp->next;
    while (temp) {
        printf("%d ", temp->data);
        temp = temp->prev;
    }
}

int main() {
    insertEnd(10); insertEnd(20); insertEnd(30);
    printf("Forward: "); displayForward();
    printf("\nBackward: "); displayBackward();
    return 0;
}
